{
  "plugins": [],
  "recurseDepth": 20,
  "opts": {
    "recurse": true,
    "destination": "./API",
    "template": "./template"
  },
  "source": {
    "include": [
      "../src"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(node_modules)"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}